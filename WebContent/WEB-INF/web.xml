<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>onetrip</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>

 <servlet>
  	<servlet-name>UserFrontController</servlet-name>
  	<servlet-class>com.onetrip.app.user.UserFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>UserFrontController</servlet-name>
  	<url-pattern>*.us</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>CityFrontController</servlet-name>
  	<servlet-class>com.onetrip.app.city.CityFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>CityFrontController</servlet-name>
  	<url-pattern>*.ci</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>AcomFrontController</servlet-name>
  	<servlet-class>com.onetrip.app.com_apply.AcomFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>AcomFrontController</servlet-name>
  	<url-pattern>*.acom</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
  	<servlet-name>PlanFrontController</servlet-name>
  	<servlet-class>com.onetrip.app.plan.PlanFrontController</servlet-class>
  </servlet>
  
   <servlet-mapping>
  	<servlet-name>PlanFrontController</servlet-name>
  	<url-pattern>*.pl</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>ScheduleFrontController</servlet-name>
  	<servlet-class>com.onetrip.app.schedule.ScheduleFrontController</servlet-class>
  </servlet>
  
   <servlet-mapping>
  	<servlet-name>ScheduleFrontController</servlet-name>
  	<url-pattern>*.sc</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>DestinationFrontController</servlet-name>
  	<servlet-class>com.onetrip.app.destination.DestinationFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>DestinationFrontController</servlet-name>
  	<url-pattern>*.de</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>MainFrontController</servlet-name>
  	<servlet-class>com.onetrip.app.main.MainFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>MainFrontController</servlet-name>
  	<url-pattern>*.m</url-pattern>
  </servlet-mapping>
  
</web-app>